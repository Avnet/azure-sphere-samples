{
    "code": "RVMonitor",
    "name": "BT510 RV Monitor",
    "authType": "1",
    "description": "Template to monitor RV temperatures",
    "isIotEdgeEnable": "0",
  "attributes": [
    {
      "name": "temp",
      "type": "NUMBER",
      "description": "Temperature as measured by the htu21d sensor",
      "unit": "degrees C"
    },
    {
      "name": "humidity",
      "type": "NUMBER",
      "description": "Releative humidity as measured by the htu21d sensor",
      "unit": "degrees C"
    },

    {
      "name": "tempBasement",
      "type": "NUMBER",
      "description": "Temperature as measured by the BT510",
      "unit": "degrees C"
    },
    {
      "name": "rssiBasement",
      "type": "NUMBER",
      "description": "Received signal strength from received BT510 message",
      "unit": "dBm"
    },
    {
      "name": "batBasement",
      "type": "NUMBER",
      "description": "Measured battery voltage",
      "unit": "V"
    },
    {
      "name": "movementBasement",
      "type": "NUMBER",
      "description": "Movement detected by BT510",
      "unit": ""
    },
    {
      "name": "magnetBasement",
